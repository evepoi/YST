    1: sw_EMU260_wagon_tail;
    sw_EMU260_wagon_front;
}

switch(FEAT_TRAINS, SELF, sw_EMU260_wagon_rear, [STORE_TEMP(1, 0x10F), var[0x61, 0, 0x0000FFFF, 0xC6]]){
