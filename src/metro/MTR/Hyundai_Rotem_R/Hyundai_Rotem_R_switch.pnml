    set_MTR_Hyundai_Rotem_R_engineH;
}

switch(FEAT_TRAINS, SELF, sw_MTR_Hyundai_Rotem_R_wagon_front, cargo_subtype) {
    1: set_MTR_Hyundai_Rotem_R_special_wagon; // 특실
    set_MTR_Hyundai_Rotem_R_Nomal_wagon;
}

switch(FEAT_TRAINS, SELF, sw_MTR_Hyundai_Rotem_R_wagon_tail, cargo_subtype) {
    1: set_MTR_Hyundai_Rotem_R_special_wagon_r;   // 특실
    set_MTR_Hyundai_Rotem_R_Nomal_wagon_r;
}

switch(FEAT_TRAINS, SELF, sw_MTR_Hyundai_Rotem_R_wagon_middle, (position_in_consist)) {
