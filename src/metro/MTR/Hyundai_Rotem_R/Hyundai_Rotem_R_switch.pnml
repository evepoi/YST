    sw_MTR_Hyundai_Rotem_R_wagon_front;
}

switch(FEAT_TRAINS, SELF, sw_MTR_Hyundai_Rotem_R_wagon_rear, [STORE_TEMP(1, 0x10F), var[0x61, 0, 0x0000FFFF, 0xC6]]){
