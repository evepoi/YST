    return string(STR_REFIT_LIVERY_MTR_Hyundai_Rotem_R_Nomal);
}

switch(FEAT_TRAINS, SELF, sw_MTR_Hyundai_Rotem_R_engine, position_in_consist_from_end == 0 && num_vehs_in_consist > 1){
